{
    "name": "endroid/teleporter",
    "description": "Endroid Teleporter",
    "keywords": ["endroid", "teleporter", "conditional", "copy", "filter"],
    "homepage": "https://github.com/endroid/teleporter",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jeroen van den Enden",
            "email": "info@endroid.nl"
        }
    ],
    "require": {
        "php": "^7.3||^8.0",
        "ext-fileinfo": "*",
        "cocur/slugify": "^4.0",
        "endroid/installer": "^1.3.2",
        "symfony/console": "^3.4||^4.4||^5.0",
        "symfony/expression-language": "^3.4||^4.4||^5.0",
        "symfony/filesystem": "^3.4||^4.4||^5.0",
        "symfony/finder": "^3.4||^4.4||^5.0",
        "twig/twig": "^2.5||^3.0"
    },
    "require-dev": {
        "endroid/quality": "dev-master"
    },
    "suggest": {
        "roave/security-advisories": "Avoids installation of package versions with vulnerabilities",
        "symfony/security-checker": "Checks your composer.lock for vulnerabilities"
    },
    "autoload": {
        "psr-4": {
            "Endroid\\Teleporter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Endroid\\Teleporter\\Tests\\": "tests/"
        }
    },
    "bin": ["bin/teleport"],
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
