{
    "name": "endroid/teleporter",
    "description": "Endroid Teleporter",
    "keywords": ["endroid", "teleporter", "conditional", "copy", "filter"],
    "homepage": "https://github.com/endroid/teleporter",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jeroen van den Enden",
            "email": "info@endroid.nl"
        }
    ],
    "require": {
        "php": ">=7.2",
        "ext-fileinfo": "*",
        "endroid/installer": "^1.0.6",
        "symfony/console": "^4.1||^5.0",
        "symfony/filesystem": "^4.1||^5.0",
        "symfony/finder": "^4.1||^5.0",
        "symfony/expression-language": "^4.1||^5.0",
        "twig/twig": "^2.5"
    },
    "require-dev": {
        "endroid/test": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Endroid\\Teleporter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Endroid\\Teleporter\\Tests\\": "tests/"
        }
    },
    "bin": ["bin/teleport"],
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
